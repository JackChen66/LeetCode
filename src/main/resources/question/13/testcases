	public void test1() {
		String inputString = "I";
		int expectedResult = 1;
		int result = romanToInteger(inputString);
		if (result == expectedResult) {
			pass++;
		} else {
			fail++;
			System.out.println(inputString + " expect integer is " + expectedResult 
							+ ", but actual is " + result);
		}
	}
	
	public void test2() {
