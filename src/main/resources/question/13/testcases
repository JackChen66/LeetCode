	static int pass = 0;
	static int fail = 0;
	static int total = 0;
	public static String errorMessage;
	public static void main(String[] args) {
		solution e = new solution();
		e.test1();
		e.test2();
		e.test3();
		e.test4();
		e.test5();
		e.test6();
		e.test7();
		e.test8();
		e.test9();
		e.test10();
		e.test11();	
		e.test12();	
		total = pass + fail;
		System.out.println("total: " + total + ", pass: " + pass + ", fail: " + fail);
		
	}